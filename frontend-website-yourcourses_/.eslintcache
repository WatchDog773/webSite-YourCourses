n  if (!app.initialized && app.minTimeElapsed) {\r\n    return (\r\n      <div>\r\n        <CardDeck> \r\n          <Card className=\"text-center m-3\">\r\n              <Card.Footer></Card.Footer>\r\n              <Navbar />\r\n              <Container>\r\n                <h1>\r\n                  ¡Oops Algo salio mal! \r\n                </h1>\r\n                <h2>No pudimos reconocerte, por favor inicia sesión</h2>\r\n                <ButtonExit contenido=\"Volver al inicio\"></ButtonExit>\r\n              </Container>\r\n            </Card>\r\n        </CardDeck> \r\n        <Footer />\r\n      </div>\r\n    );\r\n  } else {\r\n    return <section>{children}</section>;\r\n  }\r\n};\r\nconst unAuth = (dispatch, { routeLocation }) => {\r\n  return () => {\r\n    dispatch({ type: JWT_INVALID, payload: { to: routeLocation.pathname } });\r\n  };\r\n};\r\nconst appInit = async (auth, dispatch, routeHooks) => {\r\n  //Setting unAuth\r\n  setUnAuthInterceptor(unAuth(dispatch, routeHooks));\r\n\r\n  if (auth.jwt === \"\" || auth.logged === false) {\r\n    dispatch({ type: APP_INIT });\r\n  } else {\r\n    try {\r\n      setJWT(auth.jwt);\r\n      await paxios.get(\"/api/heartbeat\");\r\n      dispatch({ type: APP_INIT });\r\n    } catch (e) {\r\n      console.log(e);\r\n    }\r\n  }\r\n};\r\n\r\nexport default Splash;\r\n","/mnt/c/Users/danny/OneDrive/Documents/Github/frontend-website-yourcourses_/src/components/private/Lessons.js",[],"/mnt/c/Users/danny/OneDrive/Documents/Github/frontend-website-yourcourses_/src/components/private/AddLesson.js",[],"/mnt/c/Users/danny/OneDrive/Documents/Github/frontend-website-yourcourses_/src/utilities/Axios.js",[],"/mnt/c/Users/danny/OneDrive/Documents/Github/frontend-website-yourcourses_/src/utilities/store/reducers/app.reducer.js",[],"/mnt/c/Users/danny/OneDrive/Documents/Github/frontend-website-yourcourses_/src/utilities/store/reducers/courses.reducer.js",[],"/mnt/c/Users/danny/OneDrive/Documents/Github/frontend-website-yourcourse